generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Device {
  id        Int      @id @default(autoincrement())
  brand     String
  model     String
  price     Float?
  details   String?
  qrCode String? @db.LongText   // เพิ่มตรงนี้
  createdAt DateTime @default(now())
}


