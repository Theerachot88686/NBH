generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Device {
  id        Int     @id @default(autoincrement())
  name      String
  code      String  @unique
  brand     String
  model     String
  details   String
  createdAt DateTime @default(now())
}
