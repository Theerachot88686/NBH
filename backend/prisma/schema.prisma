generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Device {
  id         Int      @id @default(autoincrement())
  brand      String
  model      String
  price      Float?
  details    String?
  location   String?            // ðŸ‘‰ à¹€à¸žà¸´à¹ˆà¸¡à¸•à¸£à¸‡à¸™à¸µà¹‰
  qrCode     String?  @db.LongText
